    "Microsoft.DotNet.Tools.Tests.Utilities": { "target": "project" },
    "Microsoft.DotNet.Cli.Utils": {
      "target": "project",
      "type": "build"
    },

    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-dev-48273-16"
  },

  "frameworks": {
